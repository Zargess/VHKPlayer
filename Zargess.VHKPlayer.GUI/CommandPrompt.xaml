<Window x:Name="win" x:Class="Zargess.VHKPlayer.GUI.CommandPrompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:local="clr-namespace:Zargess.VHKPlayer.GUI"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CommandPrompt" Height="400" Width="700"
        AllowsTransparency="True" WindowStyle="None"
        Style="{StaticResource CustomTitleBar}">
    <Window.Background>
        <SolidColorBrush Opacity="0.8" Color="Black"/>
    </Window.Background>
    <Grid>
        <local:ConsoleControl x:Name="Cons"/>
    </Grid>
</Window>
